{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(Select-Object ProcessName, Id, StartTime)",
      "Bash(npx tsc --noEmit)",
      "Bash(for file in src/renderer/views/migration/*.tsx src/renderer/views/discovery/*.tsx)",
      "Bash(do)",
      "Bash(if grep -q \"const { source\" \"$file\")",
      "Bash(then)",
      "Bash(echo \"Fixed: $file\")",
      "Bash(fi)",
      "Bash(done)",
      "Bash(pkill -f \"electron-forge start\")",
      "Bash(taskkill //F //IM electron.exe)",
      "Bash(for file in src/renderer/views/discovery/*.tsx)",
      "Bash(pkill -f \"electron.exe\")",
      "Bash(taskkill //F //IM electron.exe //T)",
      "Bash(taskkill //F //IM node.exe //FI \"WINDOWTITLE eq npm*\" //T)",
      "Bash(tasklist)",
      "Bash(findstr /I \"electron\")",
      "Bash(tasklist //FI \"IMAGENAME eq electron.exe\")",
      "Bash(tasklist //FI \"IMAGENAME eq electron.exe\" //FO TABLE)",
      "Bash(findstr /C:\"electron\")",
      "Bash(Get-Process)",
      "Bash(Where-Object {$_rocessName -like \"*electron*\"})"
    ],
    "deny": [],
    "ask": [],
    "defaultMode": "acceptEdits",
    "additionalDirectories": [
      "C:\\enterprisediscovery",
      "D:\\d\\Scripts\\UserMandA-1",
      "C:\\discoverydata\\ljpops\\Raw",
      "C:\\discoverydata\\ljpops\\Logs",
      "D:\\d D:\\Scripts\\UserMandA",
      "C:\\EnterpriseDiscovery",
      "D:\\d\\Scripts",
      "D:\\c\\enterprisediscovery",
      "D:\\c\\discoverydata\\ljpops",
      "C:\\",
      "D:\\c\\DiscoveryData\\ljpops\\Logs",
      "D:\\",
      "D:\\d\\Scripts\\UserMandA\\GUI",
      "D:\\c\\EnterpriseDiscovery",
      "D:\\c"
    ]
  }
}
