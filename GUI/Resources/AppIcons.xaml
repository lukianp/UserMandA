<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Application Icon - Modern Discovery Theme -->
    <DrawingImage x:Key="AppIcon_Discovery">
        <DrawingImage.Drawing>
            <DrawingGroup>
                <!-- Background Circle -->
                <GeometryDrawing Brush="#2196F3">
                    <GeometryDrawing.Geometry>
                        <EllipseGeometry Center="32,32" RadiusX="30" RadiusY="30"/>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
                
                <!-- Search/Discovery Icon -->
                <GeometryDrawing Brush="White">
                    <GeometryDrawing.Geometry>
                        <PathGeometry>
                            <PathFigure StartPoint="26,20">
                                <ArcSegment Point="38,32" Size="6,6"/>
                                <ArcSegment Point="26,44" Size="6,6"/>
                                <ArcSegment Point="14,32" Size="6,6"/>
                                <ArcSegment Point="26,20" Size="6,6" IsLargeArc="True"/>
                            </PathFigure>
                        </PathGeometry>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
                
                <!-- Magnifying Glass Handle -->
                <GeometryDrawing Brush="White">
                    <GeometryDrawing.Geometry>
                        <LineGeometry StartPoint="42,42" EndPoint="50,50"/>
                    </GeometryDrawing.Geometry>
                    <GeometryDrawing.Pen>
                        <Pen Brush="White" Thickness="3"/>
                    </GeometryDrawing.Pen>
                </GeometryDrawing>
                
                <!-- Inner Detail -->
                <GeometryDrawing Brush="#1976D2">
                    <GeometryDrawing.Geometry>
                        <EllipseGeometry Center="26,32" RadiusX="3" RadiusY="3"/>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
            </DrawingGroup>
        </DrawingImage.Drawing>
    </DrawingImage>

    <!-- Application Icon - Dark Theme -->
    <DrawingImage x:Key="AppIcon_Dark">
        <DrawingImage.Drawing>
            <DrawingGroup>
                <!-- Background Circle -->
                <GeometryDrawing Brush="#37474F">
                    <GeometryDrawing.Geometry>
                        <EllipseGeometry Center="32,32" RadiusX="30" RadiusY="30"/>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
                
                <!-- Inner Circle -->
                <GeometryDrawing Brush="#546E7A">
                    <GeometryDrawing.Geometry>
                        <EllipseGeometry Center="32,32" RadiusX="24" RadiusY="24"/>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
                
                <!-- Discovery Elements -->
                <GeometryDrawing Brush="#00E5FF">
                    <GeometryDrawing.Geometry>
                        <PathGeometry>
                            <PathFigure StartPoint="20,20">
                                <LineSegment Point="44,20"/>
                                <LineSegment Point="32,44"/>
                                <LineSegment Point="20,20"/>
                            </PathFigure>
                        </PathGeometry>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
                
                <!-- Center Dot -->
                <GeometryDrawing Brush="White">
                    <GeometryDrawing.Geometry>
                        <EllipseGeometry Center="32,32" RadiusX="4" RadiusY="4"/>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
            </DrawingGroup>
        </DrawingImage.Drawing>
    </DrawingImage>

    <!-- Application Icon - Light Theme -->
    <DrawingImage x:Key="AppIcon_Light">
        <DrawingImage.Drawing>
            <DrawingGroup>
                <!-- Background Circle -->
                <GeometryDrawing Brush="White">
                    <GeometryDrawing.Geometry>
                        <EllipseGeometry Center="32,32" RadiusX="30" RadiusY="30"/>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
                
                <!-- Border -->
                <GeometryDrawing Brush="#E0E0E0">
                    <GeometryDrawing.Geometry>
                        <EllipseGeometry Center="32,32" RadiusX="30" RadiusY="30"/>
                    </GeometryDrawing.Geometry>
                    <GeometryDrawing.Pen>
                        <Pen Brush="#E0E0E0" Thickness="2"/>
                    </GeometryDrawing.Pen>
                </GeometryDrawing>
                
                <!-- Discovery Icon -->
                <GeometryDrawing Brush="#2196F3">
                    <GeometryDrawing.Geometry>
                        <PathGeometry>
                            <PathFigure StartPoint="16,16">
                                <LineSegment Point="48,16"/>
                                <LineSegment Point="48,48"/>
                                <LineSegment Point="16,48"/>
                                <LineSegment Point="16,16"/>
                            </PathFigure>
                            <PathFigure StartPoint="22,22">
                                <LineSegment Point="42,22"/>
                                <LineSegment Point="42,42"/>
                                <LineSegment Point="22,42"/>
                                <LineSegment Point="22,22"/>
                            </PathFigure>
                        </PathGeometry>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
                
                <!-- Inner Elements -->
                <GeometryDrawing Brush="#4CAF50">
                    <GeometryDrawing.Geometry>
                        <RectangleGeometry Rect="26,26,12,4"/>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
                
                <GeometryDrawing Brush="#FF9800">
                    <GeometryDrawing.Geometry>
                        <RectangleGeometry Rect="26,34,12,4"/>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
            </DrawingGroup>
        </DrawingImage.Drawing>
    </DrawingImage>

    <!-- High Contrast Icon -->
    <DrawingImage x:Key="AppIcon_HighContrast">
        <DrawingImage.Drawing>
            <DrawingGroup>
                <!-- Background -->
                <GeometryDrawing Brush="Black">
                    <GeometryDrawing.Geometry>
                        <RectangleGeometry Rect="0,0,64,64"/>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
                
                <!-- White Border -->
                <GeometryDrawing>
                    <GeometryDrawing.Geometry>
                        <RectangleGeometry Rect="4,4,56,56"/>
                    </GeometryDrawing.Geometry>
                    <GeometryDrawing.Pen>
                        <Pen Brush="White" Thickness="4"/>
                    </GeometryDrawing.Pen>
                </GeometryDrawing>
                
                <!-- Inner Content -->
                <GeometryDrawing Brush="White">
                    <GeometryDrawing.Geometry>
                        <PathGeometry>
                            <PathFigure StartPoint="16,24">
                                <LineSegment Point="32,16"/>
                                <LineSegment Point="48,24"/>
                                <LineSegment Point="48,40"/>
                                <LineSegment Point="32,48"/>
                                <LineSegment Point="16,40"/>
                                <LineSegment Point="16,24"/>
                            </PathFigure>
                        </PathGeometry>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
                
                <!-- Center Detail -->
                <GeometryDrawing Brush="Black">
                    <GeometryDrawing.Geometry>
                        <EllipseGeometry Center="32,32" RadiusX="6" RadiusY="6"/>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
            </DrawingGroup>
        </DrawingImage.Drawing>
    </DrawingImage>

    <!-- Notification Icon -->
    <DrawingImage x:Key="AppIcon_Notification">
        <DrawingImage.Drawing>
            <DrawingGroup>
                <!-- Base Icon -->
                <GeometryDrawing Brush="#2196F3">
                    <GeometryDrawing.Geometry>
                        <EllipseGeometry Center="24,24" RadiusX="22" RadiusY="22"/>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
                
                <!-- Discovery Symbol -->
                <GeometryDrawing Brush="White">
                    <GeometryDrawing.Geometry>
                        <PathGeometry>
                            <PathFigure StartPoint="12,16">
                                <LineSegment Point="36,16"/>
                                <LineSegment Point="24,32"/>
                                <LineSegment Point="12,16"/>
                            </PathFigure>
                        </PathGeometry>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
                
                <!-- Notification Badge -->
                <GeometryDrawing Brush="#FF4444">
                    <GeometryDrawing.Geometry>
                        <EllipseGeometry Center="38,10" RadiusX="8" RadiusY="8"/>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
                
                <!-- Badge Outline -->
                <GeometryDrawing>
                    <GeometryDrawing.Geometry>
                        <EllipseGeometry Center="38,10" RadiusX="8" RadiusY="8"/>
                    </GeometryDrawing.Geometry>
                    <GeometryDrawing.Pen>
                        <Pen Brush="White" Thickness="2"/>
                    </GeometryDrawing.Pen>
                </GeometryDrawing>
            </DrawingGroup>
        </DrawingImage.Drawing>
    </DrawingImage>

    <!-- Taskbar Icon -->
    <DrawingImage x:Key="AppIcon_Taskbar">
        <DrawingImage.Drawing>
            <DrawingGroup>
                <!-- Background Gradient -->
                <GeometryDrawing>
                    <GeometryDrawing.Geometry>
                        <RectangleGeometry Rect="0,0,64,64"/>
                    </GeometryDrawing.Geometry>
                    <GeometryDrawing.Brush>
                        <LinearGradientBrush StartPoint="0,0" EndPoint="1,1">
                            <GradientStop Color="#2196F3" Offset="0"/>
                            <GradientStop Color="#1976D2" Offset="1"/>
                        </LinearGradientBrush>
                    </GeometryDrawing.Brush>
                </GeometryDrawing>
                
                <!-- Main Symbol -->
                <GeometryDrawing Brush="White">
                    <GeometryDrawing.Geometry>
                        <PathGeometry>
                            <PathFigure StartPoint="32,8">
                                <LineSegment Point="56,32"/>
                                <LineSegment Point="32,56"/>
                                <LineSegment Point="8,32"/>
                                <LineSegment Point="32,8"/>
                            </PathFigure>
                        </PathGeometry>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
                
                <!-- Inner Detail -->
                <GeometryDrawing Brush="#1976D2">
                    <GeometryDrawing.Geometry>
                        <PathGeometry>
                            <PathFigure StartPoint="32,16">
                                <LineSegment Point="48,32"/>
                                <LineSegment Point="32,48"/>
                                <LineSegment Point="16,32"/>
                                <LineSegment Point="32,16"/>
                            </PathFigure>
                        </PathGeometry>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
            </DrawingGroup>
        </DrawingImage.Drawing>
    </DrawingImage>

</ResourceDictionary>